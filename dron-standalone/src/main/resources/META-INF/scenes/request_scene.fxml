<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="com.cheggnet.hackaton.controllers.RequestScene">
	<children>
		<VBox prefHeight="497.0" prefWidth="876.0">
			<children>
				<ToolBar prefHeight="40.0" prefWidth="200.0">
					<items>
						<TextField prefHeight="26.0" prefWidth="534.0">
							<padding>
								<Insets left="10.0" />
							</padding>
						</TextField>
						<ChoiceBox prefWidth="150.0">
							<items>
								<FXCollections fx:factory="observableArrayList">
									<String fx:value="GET" />
									<String fx:value="POST" />
									<String fx:value="PUT" />
									<String fx:value="DELETE" />
								</FXCollections>
							</items>
						</ChoiceBox>
						<ToggleButton mnemonicParsing="false" onAction="#onSelectCB" text="URL params" />
						<ToggleButton mnemonicParsing="false" text="Headers" />
					</items>
				</ToolBar>
				<VBox prefHeight="0" prefWidth="876.0">
					<children>
						<HBox prefHeight="12.0" prefWidth="876.0">
							<children>
								<TextField />
								<TextField>
									<HBox.margin>
										<Insets left="25.0" />
									</HBox.margin>
								</TextField>
								<Button mnemonicParsing="false" text="Delete">
									<HBox.margin>
										<Insets left="25.0" />
									</HBox.margin>
								</Button>
							</children>
							<padding>
								<Insets bottom="5.0" left="10.0" top="5.0" />
							</padding>
						</HBox>
					</children>
				</VBox>
				<HBox prefHeight="30.0" prefWidth="878.0" spacing="2.0">
					<children>
						<Button mnemonicParsing="false" prefHeight="25.0"
							prefWidth="50.0" text="Send" />
						<Button mnemonicParsing="false" prefHeight="26.0"
							prefWidth="71.0" text="Preview" />
						<Button mnemonicParsing="false" prefHeight="26.0"
							prefWidth="137.0" text="Add to collection" />
					</children>
					<padding>
						<Insets bottom="2.0" left="2.0" top="2.0" />
					</padding>
				</HBox>
				<TextArea prefHeight="427.0" prefWidth="876.0" />
			</children>
		</VBox>
	</children>
</AnchorPane>
